{% block html_head_container %}
    {% include 'html-header.twig' %}
    {% block head %}
    {% endblock %}
    </head>
{% endblock %}

    <body class="{{body_class}} wf-loading" data-template="base.twig" id="pageTop">
        
        <div class="invisible">
            {% include 'partials/svg-defs.svg' %}    
        </div>
        
        <header class="site-header" role="banner">
            {% block header %}
                {% include 'headers/header-site.twig' %}
            {% endblock %}
        </header>
        
        <main id="content" class="site-main" role="main">
            
            <div class="page-body">
                {% block content %}
                    Sorry, no content
                {% endblock %}
            </div>

            {% if sidebar %}
                <aside class="layout-sidebar">
                    {{sidebar}}
                </aside>
            {% endif %}

        </main>

        {% block footer %}
            
            <nav class="link-back-top">
                {# <div id="back-top"> #}
                    <a href="#top">
                        {% include 'partials/svg.twig' with {'icon': 'arrow-up', 'prefix': 'icon' } %}
                    </a>
                {# </div> #}
            </nav>

            <footer class="site-footer" role="contentinfo">
                <div class="contain-lg">
                    {% include 'footer.twig' %}    
                </div>
            </footer>

            {{ function('wp_footer') }}
            
        {% endblock %}

    </body>

</html>