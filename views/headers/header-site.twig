
<div class="site-logo">
  <a class="site-logo-link" href="{{site.link}}">
    {% include "svgs/lara-schenck-horiz.svg" %}
    <span class="screen-reader">Lara Schenck</span>
  </a>
</div>

<nav id="menu" class="site-nav" role="navigation">
  <ul class="menu-list">
    {% for item in main_nav.items %}
    <li class="menu-item {{ item.get_children ? 'menu-has-children' }} {{item.classes | join(' ')}}">
      <a class="menu-item-link" href="{{item.link}}">{{item.title}}</a>
    </li>
    {% endfor %}
  </ul>
</nav><!-- /#menu -->

<div class="site-icon">
  {% include "svgs/character-1.svg" %}
</div>
